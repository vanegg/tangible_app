<div class="display_photos">
  <% if current_user.photos.empty? %>
    <span>"¡Oops! Aún no tienes fotos. ¡Súbelas de tu dispositivo o Instagram!"</span>
  <% else %>
    <!-- <div id="prompt-new-album"></div> -->
    <% current_user.photos.reverse.each do |photo| %>  
        <img id="<%= photo.photo_url(:large)%>" src="<%= photo.photo_url(:thumb) %>">
    <% end %>
  <% end %>
</div>